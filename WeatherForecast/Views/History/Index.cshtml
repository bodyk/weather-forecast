
@model ICollection<WeatherForecast.Models.RequestHistoryEntity>

    @{
        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

    <h2>Index</h2>

    <table>
        <thead>
            <tr>
                <td>Time</td>
            </tr>
            <tr>
                <td>RequestInfo</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var historyItem in Model)
            {
                <tr>
                    <td>
                        @historyItem.RequestTime
                    </td>
                    <td>
                        @historyItem.WeatherEntities.Count
                    </td>
                </tr>
            }
        </tbody>
    </table>